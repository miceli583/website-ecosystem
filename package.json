  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "clean": "rm -rf .next",
    "clean:all": "rm -rf .next node_modules/.cache",
    "clear-auth": "node scripts/clear-auth-state.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbo",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "env:validate": "node scripts/setup-env.js validate",
    "env:setup": "node scripts/setup-env.js setup",
    "pre-commit": "bash scripts/pre-commit.sh",
    "quality-check": "npm run format && npm run lint:fix && npm run lint && npm run typecheck && npm run build",
    "generate-resume-pdf": "tsx scripts/generate-resume-pdf.ts",
